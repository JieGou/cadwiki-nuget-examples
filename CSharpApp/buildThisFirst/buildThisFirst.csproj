  <Target Name="autoincrementBuildThisFirstAssemblyInfo" BeforeTargets="Build">
    <Exec Command="echo target for auto incrementing AssemblyInfo.vb files" />
    <!-- Autoincrement buildThisFirst AssemblyInfo-->
    <GetAssemblyIdentity AssemblyFiles="$(TargetPath)">
      <Output TaskParameter="Assemblies" ItemName="buildThisFirstAssemblyIdentities" />
    </GetAssemblyIdentity>
    <Exec Command="echo project buildThisFirst current assembly version is: %(buildThisFirstAssemblyIdentities.Version)" />
    <PropertyGroup>
      <Pattern>(\d+)(.)(\d+)(.)(\d+)(.)(\d+)</Pattern>
      <In>%(buildThisFirstAssemblyIdentities.Version)</In>
      <Out1>$([System.Text.RegularExpressions.Regex]::Match($(In), $(Pattern), System.Text.RegularExpressions.RegexOptions.Multiline).Groups[1].Value)</Out1>
      <Out2>$([System.Text.RegularExpressions.Regex]::Match($(In), $(Pattern), System.Text.RegularExpressions.RegexOptions.Multiline).Groups[3].Value)</Out2>
      <Out3>$([System.Text.RegularExpressions.Regex]::Match($(In), $(Pattern), System.Text.RegularExpressions.RegexOptions.Multiline).Groups[5].Value)</Out3>
      <Out4>$([System.Text.RegularExpressions.Regex]::Match($(In), $(Pattern), System.Text.RegularExpressions.RegexOptions.Multiline).Groups[7].Value)</Out4>
    </PropertyGroup>
    <ItemGroup>
      <Reference Include="Microsoft.VisualBasic" />
      <AssemblyInfoFiles Include="$(BuildRoot)\**\AssemblyInfo.cs" />
    </ItemGroup>
    <AssemblyInfo AssemblyInfoFiles="@(AssemblyInfoFiles)" AssemblyMajorVersion="$(Out1)" AssemblyMinorVersion="$(Out2)" AssemblyBuildNumberType="DateString" AssemblyBuildNumberFormat="MMdd" AssemblyRevisionType="AutoIncrement" AssemblyRevisionFormat="000" />
    <AssemblyInfo AssemblyInfoFiles="@(AssemblyInfoFiles)" AssemblyFileMajorVersion="$(Out1)" AssemblyFileMinorVersion="$(Out2)" AssemblyFileBuildNumberType="DateString" AssemblyFileBuildNumberFormat="MMdd" AssemblyFileRevisionType="AutoIncrement" AssemblyFileRevisionFormat="000" />
    <Exec Command="echo target incremented assembly version for all assemblies in solution." />
    <!-- print statement for debugging -->
    <!-- <Message Text="@(AssemblyInfoFiles, '%0a')" Importance="high" /> -->
  </Target>
